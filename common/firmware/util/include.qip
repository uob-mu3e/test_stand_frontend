#

# include sdc files
foreach f [ lsort [ glob -directory $::quartus(qip_path) \
    "*.sdc" \
] ] {
    set_global_assignment -name SDC_FILE $f
}

# include vhd files
foreach f [ lsort [ glob -directory $::quartus(qip_path) \
    "*.vhd" \
    "8b10b/*.vhd" \
] ] {
    set_global_assignment -name VHDL_FILE $f
}
